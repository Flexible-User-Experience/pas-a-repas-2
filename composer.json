{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.4.0",
        "ext-ctype": "*",
        "ext-fileinfo": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "artgris/filemanager-bundle": "^2.0",
        "beberlei/doctrineextensions": "^1.0",
        "digitick/sepa-xml": "^1.6",
        "doctrine/common": "^2.13",
        "doctrine/doctrine-migrations-bundle": "^2.1",
        "drewm/mailchimp-api": "^2.5",
        "egulias/email-validator": "^2.1",
        "excelwebzone/recaptcha-bundle": "^1.5",
        "friendsofsymfony/ckeditor-bundle": "^2.2",
        "friendsofsymfony/jsrouting-bundle": "^2.5",
        "gremo/zurb-ink-bundle": "^4.1",
        "knplabs/knp-menu-bundle": "^2.0",
        "knplabs/knp-paginator-bundle": "^3.0",
        "kunicmarko/color-picker-bundle": "^1.1",
        "liip/imagine-bundle": "^2.1",
        "presta/sitemap-bundle": "^2.1",
        "qipsius/tcpdf-bundle": "^2.0",
        "saad-tazi/g-chart-bundle": "^3.1",
        "sensio/framework-extra-bundle": "^5.5",
        "sonata-project/admin-bundle": "^3.70",
        "sonata-project/doctrine-orm-admin-bundle": "^3.10",
        "sonata-project/user-bundle": "^4.0",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/intl": "4.4.*",
        "symfony/mailer": "4.4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer-pack": "*",
        "symfony/translation": "4.4.*",
        "symfony/twig-pack": "^1.0",
        "symfony/validator": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "4.4.*",
        "tattali/calendar-bundle": "^1.2",
        "tecnickcom/tcpdf": "^6.2.17",
        "twig/cssinliner-extra": "^3.0",
        "twig/inky-extra": "^3.0",
        "twig/markdown-extra": "^3.0",
        "vich/uploader-bundle": "1.13.1"
    },
    "require-dev": {
        "hautelook/alice-bundle": "^2.7",
        "sensiolabs/security-checker": "^6.0",
        "symfony/debug-pack": "*",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*"
        }
    }
}
